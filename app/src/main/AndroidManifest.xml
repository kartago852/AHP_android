<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.spk.ahp_lokasipabrikbambu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".input.KriteriaActivity"
            android:label="Criterios"
            android:screenOrientation="portrait" />
        <activity
            android:name=".input.AlternatifActivity"
            android:label="Alternativas"
            android:screenOrientation="portrait" />
        <activity
            android:name=".bobot.KriteriaBobotActivity"
            android:label="Importancia de Criterios"
            android:screenOrientation="portrait" />
        <activity
            android:name=".result.KriteriaBobotResultActivity"
            android:label="Resultados de Matriz de Criterios"
            android:screenOrientation="portrait" />
        <activity
            android:name=".bobot.AlternatifBobotActivity"
            android:label="Importancia de Alternativas"
            android:screenOrientation="portrait" />
        <activity
            android:name=".result.AlternatifBobotResultActivity"
            android:label="Resultados de Matriz de Alternativas"
            android:screenOrientation="portrait" />
    </application>

</manifest>